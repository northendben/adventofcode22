import pdb
import time

score_dictionary = {
"A": 1, #Rock
"B": 2, #Paper
"C": 3  #Scissors
}

my_score = 0
opponent_score = 0

for item in strategy_guide:
    opponent_choice = item[0]
    result = item[1]
    if result == "X":
        if opponent_choice == "A":
            my_choice = "C"
        elif opponent_choice == "B":
            my_choice = "A"
        else:
            my_choice = "B"
        opponent_score += 6 + score_dictionary[opponent_choice]
        my_score += score_dictionary[my_choice]

    elif result == "Y":
        if opponent_choice == "A":
            my_choice ="A"
        elif opponent_choice == "B":
            my_choice ="B"
        else:
            my_choice = "C"
        opponent_score +=3 + score_dictionary[opponent_choice]
        my_score +=3 + score_dictionary[my_choice]

    elif result == "Z":
        if opponent_choice == "A":
            my_choice = "B"
        elif opponent_choice =="B":
            my_choice = "C"
        else:
            my_choice = "A"
        opponent_score += score_dictionary[opponent_choice]
        my_score +=6 + score_dictionary[my_choice]
print(my_score)
